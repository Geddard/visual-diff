{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "declaration": true,
    "declarationDir": "./dist/types",
    "esModuleInterop": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "dist",
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "ESNext",
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
